<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.progra.vistas.actividades.ActividadRecibirParametros">

    <TextView
        android:id="@+id/lblNombreRecibirParametro"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:text="El nombre es: "
        android:textSize="24sp" />

    <TextView
        android:id="@+id/lblApellidoRecibirParametro"
        android:layout_width="match_parent"
        android:layout_height="63dp"
        android:text="El apellido es: "
        android:textSize="24sp" />


</LinearLayout>